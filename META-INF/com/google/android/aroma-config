### FILE INFO:
#
# AROMA Installer 2.70 - AROMA Installer Test Script Configuration
#       (c) 2011-2012 by Ahmad Amarullah
#           amarullz - xda-developers
#           http://www.amarullz.com/
#
#       Binary Version  : 2.70B
#       Binary Codename : Flamboyan
#
### INFO:
#
# * AROMA Resource Dir  = META-INF/com/google/android/aroma
#
# * AROMA Temporary Dir = /tmp/aroma/
#
# * About Icon          = @alert - show <AROMA Resource Dir>/icons/alert.png
#                         or if theme used it will find <themedir>/icon.alert.png
#                         you can still access it via "icons/alert"
#
# * dp                  = Device Pixelate. WVGA/QHD = 3, HVGA = 2.
#                         dp calculated with = round(min(width,height) / 160)
#
###

##
# Fix Colorspace Issue
#
# For device which have a problem with color ( not display the correct color )
# try to set "force_colorspace" with ini_set. It may caused by wrong framebuffer
# information that passed by kernel into framebuffer device.
#
# You can use one of following values:
#   "rgba", "abgr", "argb", "bgra"
#
# NOTE: It must be in top most script, or you will got the color flicker
#
#ini_set("force_colorspace","argb");
#

##
# Forcing Device Pixelate Size
#  defaut: floor( min( width, height ) / 160 )
#
# Usefull for tablet device, if interface was too big, you can
# force dp size for smaller UI size.
#
#-------- Uncomment one of this to use custom dp size:
#
# ini_set("dp","1"); #-- LDPI ( 240x320 / QVGA )
# ini_set("dp","2"); #-- MDPI ( 340x480 / HVGA )
# ini_set("dp","3"); #-- HDPI ( 480x800 / WVGA )
# ini_set("dp","4");
# ini_set("dp","5");
#

##
# Initializing Rom Information
#
ini_set("rom_name",             "ImproveG3");
ini_set("rom_author",           "subworx");
ini_set("rom_version",          "1.2");
ini_set("rom_device",           "LG G3");
ini_set("rom_date",             "2015.09.16");


#
# Set Small Font to Support all fonts
#
fontresload( "0", "ttf/Roboto-Regular.ttf", "12" ); #-- Use sets of font (Font Family)
fontresload( "1", "ttf/Roboto-Regular.ttf", "14" ); #-- Use sets of font (Font Family)

#
# INCLUDE EXAMPLE = Language Selection & Form Example
#
include("language_select.edify");
#include("custombox_test.edify");

#########################################################################################################
#
# STEP 1 - SHOW ROM INFORMATION
#
viewbox(
  #-- Title
    "<~welcome.title>",
  
  #-- Text
    "<~welcome.text1> <b>"+
	  #-- Get Config Value
	  ini_get("rom_name")+
	"</b> <~common.for> <b>"+ini_get("rom_device")+"</b>.\n\n"+
    
    "<~welcome.text2>\n\n"+
	
      "  <~welcome.version>\t\t: <b><#selectbg_g>"+ini_get("rom_version")+"</#></b>\n"+
      "  <~welcome.updated>\t\t: <b><#selectbg_g>"+ini_get("rom_date")+"</#></b>\n"+
	  "  <~welcome.creator>\t\t: <b><#selectbg_g>"+ini_get("rom_author")+"</#></b>\n\n\n"+
	  
    "<~welcome.next>",

  #-- Icon
    "@welcome"
);

#########################################################################################################
#
# STEP 2 - WARNING
#
agreebox(
  #-- Title
    "<~warning.title>",
  
  #-- Subtitle / Description
    "<~warning.desc>",
  
  #-- Icon
    "@alert",
  
  #-- Text Content ( Read from <AROMA Resource Dir>/license.txt )
    "<~warning.text>",
  
  #-- Checkbox Text
    "<~warning.check>",
  
  #-- Unchecked Alert Message
    "<~warning.confirm>"
);


#########################################################################################################
#
# STEP 3 - CHANGELOG
#
textbox(
  #-- Title
    "<~changelog.title>",
  
  #-- Subtitle
    "<~changelog.desc>",
  
  #-- Icon
    "@update",
  
  #-- Arg 4
    resread("changelog.txt");
);

##
##-- Use Roboto-Regular.ttf
##
fontresload( "0", "ttf/Roboto-Regular.ttf", "12" );
fontresload( "1", "ttf/Roboto-Regular.ttf", "14" );
##
##


#
# Save Demo Menu Position
#
gotolabel("aroma_installer_demo_pos");

#########################################################################################################
#
# STEP 4 - SHOW MENUBOX - AROMA INSTALLER DEMO MENU
#
menubox(
  #-- Title
    "<~mainmenu.title>",
  
  #-- Sub Title
    "<~mainmenu.desc>",
  
  #-- Icon
    "@apps",
    
  #-- Will be saved in /tmp/aroma/demo.prop
    "demo.prop",
    
  #-------------------------+-----------[ Menubox Items ]-------------------+---------------#
  # TITLE                   |  SUBTITLE                                     |   Item Icons  #
  #-------------------------+-----------------------------------------------+---------------#
  
    "<~mainmenu.removeapps>", "<~mainmenu.removedesc>",                      "@customize",   #-- selected = 1
	"<~mainmenu.sound>",      "<~mainmenu.sounddesc>",                       "@customize",   #-- selected = 2
	"<~mainmenu.sdfix>",	  "<~mainmenu.sdfixdesc>",						 "@customize",	 #-- selected = 3
	"<~mainmenu.cttmod>",     "<~mainmenu.cttdesc>", 						 "@customize",   #-- selected = 4
	"<~mainmenu.sharp>",	  "<~mainmenu.sharpdesc>",						 "@customize",	 #-- selected = 5
	"<~mainmenu.systeminfo>", "<~mainmenu.systemdesc>",         			 "@info"         #-- selected = 6
);



#########################################################################################################
#                                                                                                       #
#                                        STEP 5 - ACTIONS                                               #
#                                                                                                       #
#########################################################################################################

##--------------------------------------
#
# System Info Demo / selected 6
#
if prop("demo.prop","selected")=="6" then

  #-- Show Please Wait
  pleasewait("Get System Informations...");

  #-- Fetch System Information
  setvar(
    #-- Variable Name
      "sysinfo",
    
    #-- Variable Value
      "<@center><b>Your Device System Information</b></@>\n\n"+
      
      "Model\t\t\t: <b><#selectbg_g>"+sysprop("ro.product.model")+"</#></b>\n"+
      "Name\t\t\t: <b><#selectbg_g>"+sysprop("ro.product.name")+"</#></b>\n"+
      "Device Name\t\t: <b><#selectbg_g>"+sysprop("ro.product.device")+"</#></b>\n"+
      "Board Name\t\t: <b><#selectbg_g>"+sysprop("ro.product.board")+"</#></b>\n"+
      "Manufacturer\t\t: <b><#selectbg_g>"+sysprop("ro.product.manufacturer")+"</#></b>\n\n"+
      
      "CPU\t\t\t: <b><#selectbg_g>"+sysprop("ro.product.cpu.abi")+"</#></b>\n"+
      "LCD Desinty\t\t: <b><#selectbg_g>"+sysprop("ro.sf.lcd_density")+"</#></b>\n\n"+
      
      "System Size\t\t: <b><#selectbg_g>"+getdisksize("/system","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/system","m")+" MB</#></b>\n\n"+
      "Data Size\t\t: <b><#selectbg_g>"+getdisksize("/data","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/data","m")+" MB</#></b>\n\n"+
      "(/mnt/sdcard)\t\t: <b><#selectbg_g>"+getdisksize("/mnt/sdcard","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/mnt/sdcard","m")+" MB</#></b>\n\n"+
      "SDCard Size\t\t: <b><#selectbg_g>"+getdisksize("/sdcard","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/sdcard","m")+" MB</#></b>\n\n"+
      "SD-EXT Size\t\t: <b><#selectbg_g>"+getdisksize("/sd-ext","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/sd-ext","m")+" MB</#></b>\n\n"+
	  "(/cust)\t\t: <b><#selectbg_g>"+getdisksize("/cust","m")+" MB</#></b>\n"+
        "\tFree\t\t: <b><#selectbg_g>"+getdiskfree("/cust","m")+" MB</#></b>\n\n"+
      

      ""
  );
  
  #-- Show Textbox
  textbox(
    #-- Title
      "System Information",
    
    #-- Subtitle
      "Current system information may not be supported in all devices.",
    
    #-- Icon
      "@info",
    
    #-- Text
      getvar("sysinfo")
  );
  
  #-- Show Alert
  alert(
    #-- Alert Title
      "Finished",
    
    #-- Alert Text
      "Returning to Main Menu",
    
    #-- Alert Icon
      "@alert"
  );
  
  #-- Back to Demo Menu ( 2 Wizard UI to Back )
  back("2");
  
endif;

##--------------------------------------
#
# Oversharpening Fix / selected 5
#
if prop("demo.prop","selected")=="5" then

  #########################################################################################################
  #                                                                                                       #
  #                                       SHARPENING MOD                                                  #
  #                                                                                                       #
  #########################################################################################################

  #########################################################################################################
  #
  # SHARPENING MOD STEP 1 - SPLASH
  #

  ##
  # Show Simple Splash
  #
  splash(
    #-- Duration 3000ms / 3 seconds
    3000,
    
    #-- <AROMA Resource Dir>/oversharpenlogo.png
    "oversharpenlogo"
  );

  #########################################################################################################
  #
  # SHARPENING MOD STEP 2 - INSTRUCTIONS
  #
  textbox(
    #-- Title
      "<~sharpinstructions.title>",
  
    #-- Subtitle
      "<~sharpinstructions.desc>",
  
    #-- Icon
      "@alert",
  
    #-- Arg 4
      "<~sharpinstructions.warning>"
  );

  #########################################################################################################
  #
  # SHARPENING MOD STEP 3 - SELECT ACTION
  #
  selectbox(
  #-- Title
    "<~sharpcustomize.title>",
  
  #-- Sub Title
    "<~sharpcustomize.desc>",
  
  #-- Icon: <AROMA Resource Dir>/icons/personalize.png or <ThemeDir>/icon.personalize.png
    "@personalize",
  
  #-- Will be saved in /tmp/aroma/sharp.prop
    "sharp.prop",
  
  #-----------------[ Selectbox Without Group ]-------------------#
  # TITLE  |  SUBTITLE                            | Initial Value #
  #--------+--------------------------------------+---------------#

    "00",    "Sharpening Disabled (Off)",          1,      #-- selected.0 = 1
    "01",    "Sharpening set to 01",               0,      #-- selected.0 = 2
    "02",    "Sharpening set to 02",               0,      #-- selected.0 = 3
    "03",    "Sharpening set to 03",               0,      #-- selected.0 = 4
    "04",    "Sharpening set to 04",               0,      #-- selected.0 = 5
    "05",    "Sharpening set to 05",               0,      #-- selected.0 = 6
    "06",    "Sharpening set to 06",               0,      #-- selected.0 = 7
    "07",    "Sharpening set to 07",               0,      #-- selected.0 = 8
    "08",    "Sharpening set to 08",               0,      #-- selected.0 = 9
    "09",    "Sharpening set to 09",               0,      #-- selected.0 = 10
    "10",    "Sharpening set to 10",               0,      #-- selected.0 = 11
    "11",    "Sharpening set to 11",               0,      #-- selected.0 = 12
    "12",    "Sharpening set to 12",               0,      #-- selected.0 = 13
    "13",    "Sharpening set to 13",               0,      #-- selected.0 = 14
    "14",    "Sharpening set to 14",               0,      #-- selected.0 = 15
    "15",    "Sharpening set to 15",               0,      #-- selected.0 = 16
    "16",    "Sharpening set to 16",               0,      #-- selected.0 = 17
    "17",    "Sharpening set to 17",               0,      #-- selected.0 = 18
    "18",    "Sharpening set to 18",               0,      #-- selected.0 = 19
    "19",    "Sharpening set to 19",               0,      #-- selected.0 = 20
    "20",    "Sharpening set to 20",               0,      #-- selected.0 = 21
    "21",    "Sharpening set to 21",               0,      #-- selected.0 = 22
    "22",    "Sharpening set to 22",               0,      #-- selected.0 = 23
    "23",    "Sharpening set to 23",               0,      #-- selected.0 = 24
    "24",    "Sharpening set to 24",               0,      #-- selected.0 = 25
    "25",    "Sharpening set to 25",               0,      #-- selected.0 = 26
    "26",    "Sharpening set to 26",               0,      #-- selected.0 = 27
    "27",    "Sharpening set to 27",               0,      #-- selected.0 = 28
    "28",    "Sharpening set to 28",               0,      #-- selected.0 = 29
    "29",    "Sharpening set to 29",               0,      #-- selected.0 = 30
    "30",    "Sharpening set to 30",               0,      #-- selected.0 = 31
    "31",    "Sharpening set to 31",               0,      #-- selected.0 = 32
    "32",    "Sharpening set to 32",               0,      #-- selected.0 = 33
    "33",    "Sharpening set to 33",               0,      #-- selected.0 = 34
    "34",    "Sharpening set to 34",               0,      #-- selected.0 = 35
    "35",    "Sharpening set to 35",               0,      #-- selected.0 = 36
    "36",    "Sharpening set to 36",               0,      #-- selected.0 = 37
    "37",    "Sharpening set to 37",               0,      #-- selected.0 = 38
    "38",    "Sharpening set to 38",               0,      #-- selected.0 = 39
    "39",    "Sharpening set to 39",               0,      #-- selected.0 = 40
    "40",    "Sharpening set to 40 (Stock)",       0       #-- selected.0 = 41

  #--------[ Initial Value = 0: Unselected, 1: Selected, 2: Group Item, 3: Not Visible ]---------#
  );
  
  ##
  #
  # Next Screen is Installation UI, So change the next button text to "Install Now"
  #
  ini_set("text_next", "<~sharpcustomize.nextbutton>");
  ini_set("icon_next", "@installbutton");

  ###################################################################################################################
  #
  # SHARPENING MOD STEP 4 - MAKE SURE USER KNOW THAT NEXT STEP WAS INSTALLATION PROCESS
  #
  viewbox(
    #-- Title
      "<~sharpconfirmation.title>",

    #-- Text
      "<~sharpconfirmation.text>",

    #-- Icon
      "@install"
  );

  ##
  #
  # Next Button in successfull installation should be back to "Next >"
  #
  ini_set("text_next", "<~text_next>");
  ini_set("icon_next", "@next");

  #-- Extract sleep to /tmp/aroma/sleep. It will be used in updater-script for testing wait script
  restotmp("exec_demo/sleep","sleep");

  ###################################################################################################################
  #
  # SHARPENING MOD STEP 5 - INSTALLATION PROCESS
  #
  setvar("retstatus",
    install(
	  #-- Title
        "<~sharpinstall.title>",
	
	  #-- Installation Process message
	    "<b>"+ini_get("rom_name")+"</b> <~sharpinstall.desc>",
	
	  #-- Installation Icon
        "@install",
	
	  #-- Installation Finish Message
        "<~sharpinstall.finish>"
    )
  );

  ###################################################################################################################
  #
  # SHARPENING MOD STEP 6 - ASK If We Want to go back to main menu
  #
  checkviewbox(
    #-- Title
      "<~sharppostinstall.title>",
  
    #-- Text
      "<~sharppostinstall.text>",
	
    #-- Icon
      "@welcome",

    #-- Checkbox Text
      "<~sharppostinstall.checkbox>",

    #-- Initial Checkbox value ( 0=unchecked, 1=checked ) -  (Optional, default:0)
      "0",

    #-- Save checked value in variable "reboot_it" (Optional)
      "goto_mainmenu"
  );
  if
    getvar("goto_mainmenu")=="1"
  then
    goto("aroma_installer_demo_pos");
  endif;
  
  ##########
  #
  # END SHARPENING MOD
  #

endif;

##--------------------------------------
#
# Temperature Mod / selected 4
#
if prop("demo.prop","selected")=="4" then
  #########################################################################################################
  #                                                                                                       #
  #                                            CTT MOD                                                    #
  #                                                                                                       #
  #########################################################################################################

  #########################################################################################################
  #
  # CTT MOD STEP 1 - INSTRUCTIONS
  #
  textbox(
    #-- Title
      "<~cttinstructions.title>",
  
    #-- Subtitle
      "<~cttinstructions.desc>",
  
    #-- Icon
      "@alert",
  
    #-- Arg 4
      "<~cttinstructions.warning>"
  );
  agreebox(
  #-- Title
    "<~cttinstructions.title>",
  
  #-- Subtitle / Description
    "<~cttagree.desc>",
  
  #-- Icon
    "@alert",
  
  #-- Text Content ( Read from <AROMA Resource Dir>/license.txt )
    "<~cttagree.text>",
  
  #-- Checkbox Text
    "<~cttagree.check>",
  
  #-- Unchecked Alert Message
    "<~cttagree.confirm>"
  );

  #########################################################################################################
  #
  # CTT MOD STEP 2 - SELECT ACTION
  #
  selectbox(
  #-- Title
    "<~cttcustomize.title>",
  
  #-- Sub Title
    "<~cttcustomize.desc>",
  
  #-- Icon: <AROMA Resource Dir>/icons/personalize.png or <ThemeDir>/icon.personalize.png
    "@customize",
  
  #-- Will be saved in /tmp/aroma/ctt.prop
    "ctt.prop",
  
  #-------------------------[ Selectbox Without Group ]--------------------------#
  # TITLE                 |  SUBTITLE                            | Initial Value #
  #-----------------------+--------------------------------------+---------------#

    "<~cttcustomize.1>",    "<~cttcustomize.1desc>",               1,      #-- selected.0 = 1, stock
    "<~cttcustomize.2>",    "<~cttcustomize.2desc>",               0,      #-- selected.0 = 2, stock 60
    "<~cttcustomize.3>",    "<~cttcustomize.3desc>",               0,      #-- selected.0 = 3, stock 65
    "<~cttcustomize.4>",    "<~cttcustomize.4desc>",               0,      #-- selected.0 = 4, stock 70
    "<~cttcustomize.5>",    "<~cttcustomize.5desc>",               0,      #-- selected.0 = 5, stock 75
	"<~cttcustomize.6>",    "<~cttcustomize.6desc>",               0,      #-- selected.0 = 6, stock 75 more bumps
	"<~cttcustomize.7>",    "<~cttcustomize.7desc>",               0,      #-- selected.0 = 1, stepdown 70
	"<~cttcustomize.8>",    "<~cttcustomize.8desc>",               0,      #-- selected.0 = 1, stepdown 75
	"<~cttcustomize.9>",    "<~cttcustomize.9desc>",               0,      #-- selected.0 = 1, stepdown 80
	"<~cttcustomize.10>",   "<~cttcustomize.10desc>",              0,      #-- selected.0 = 1, OC 75
	"<~cttcustomize.11>",   "<~cttcustomize.11desc>",              0,      #-- selected.0 = 1, chupa only? 70
	"<~cttcustomize.12>",   "<~cttcustomize.12desc>",              0,      #-- selected.0 = 1, chupa 70
	"<~cttcustomize.13>",   "<~cttcustomize.13desc>",              0       #-- selected.0 = 1, cat6 70
		
  #--------[ Initial Value = 0: Unselected, 1: Selected, 2: Group Item, 3: Not Visible ]---------#
  );
  
  ##
  #
  # Next Screen is Installation UI, So change the next button text to "Install Now"
  #
  ini_set("text_next", "<~cttcustomize.nextbutton>");
  ini_set("icon_next", "@installbutton");

  ###################################################################################################################
  #
  # CTT MOD STEP 3 - MAKE SURE USER KNOW THAT NEXT STEP WAS INSTALLATION PROCESS
  #
  viewbox(
    #-- Title
      "<~cttconfirmation.title>",

    #-- Text
      "<~cttconfirmation.text>",

    #-- Icon
      "@install"
  );

  ##
  #
  # Next Button in successfull installation should be back to "Next >"
  #
  ini_set("text_next", "<~text_next>");
  ini_set("icon_next", "@next");

  #-- Extract sleep to /tmp/aroma/sleep. It will be used in updater-script for testing wait script
  restotmp("exec_demo/sleep","sleep");

  ###################################################################################################################
  #
  # CTT MOD STEP 4 - INSTALLATION PROCESS
  #
  setvar("retstatus",
    install(
	  #-- Title
        "<~cttinstall.title>",
	
	  #-- Installation Process message
	    "<b>"+ini_get("rom_name")+"</b> <~cttinstall.desc>",
	
	  #-- Installation Icon
        "@install",
	
	  #-- Installation Finish Message
        "<~cttinstall.finish>"
    )
  );

  ###################################################################################################################
  #
  # CTT MOD STEP 5 - ASK If We Want to go back to main menu
  #
  checkviewbox(
    #-- Title
      "<~cttpostinstall.title>",
  
    #-- Text
      "<~cttpostinstall.text>",
	
    #-- Icon
      "@finish",

    #-- Checkbox Text
      "<~cttpostinstall.checkbox>",

    #-- Initial Checkbox value ( 0=unchecked, 1=checked ) -  (Optional, default:0)
      "0",

    #-- Save checked value in variable "reboot_it" (Optional)
      "goto_mainmenu"
  );
  if
    getvar("goto_mainmenu")=="1"
  then
    goto("aroma_installer_demo_pos");
  endif;
  
  ##########
  #
  # END CTT MOD
  #

endif;



  ##
  #
  # Next Screen is Installation UI, So change the next button text to "Install Now"
  #
  ini_set("text_next", "<~debloatcustomize.nextbutton>");
  ini_set("icon_next", "@installbutton");

  ###################################################################################################################
  #
  # DEBLOATER STEP 4 - MAKE SURE USER KNOW THAT NEXT STEP WAS INSTALLATION PROCESS
  #
  viewbox(
    #-- Title
      "<~debloatconfirmation.title>",

    #-- Text
      "<~debloatconfirmation.text>",

    #-- Icon
      "@install"
  );

  ##
  #
  # Next Button in successfull installation should be back to "Next >"
  #
  ini_set("text_next", "<~text_next>");
  ini_set("icon_next", "@next");

  #-- Extract sleep to /tmp/aroma/sleep. It will be used in updater-script for testing wait script
  restotmp("exec_demo/sleep","sleep");

  ###################################################################################################################
  #
  # DEBLOATER STEP 5 - INSTALLATION PROCESS
  #
  setvar("retstatus",
    install(
	  #-- Title
        "<~debloatinstall.title>",
	
	  #-- Installation Process message
	    "<b>"+ini_get("rom_name")+"</b> <~debloatinstall.desc>",
	
	  #-- Installation Icon
        "@install",
	
	  #-- Installation Finish Message
        "<~debloatinstall.finish>"
    )
  );

  ###################################################################################################################
  #
  # DEBLOATER STEP 6 - ASK If We Want to go back to main menu
  #
  checkviewbox(
    #-- Title
      "<~debloatpostinstall.title>",
  
    #-- Text
      "<~debloatpostinstall.text>",
	
    #-- Icon
      "@finish",

    #-- Checkbox Text
      "<~debloatpostinstall.checkbox>",

    #-- Initial Checkbox value ( 0=unchecked, 1=checked ) -  (Optional, default:0)
      "0",

    #-- Save checked value in variable "reboot_it" (Optional)
      "goto_mainmenu"
  );
  if
    getvar("goto_mainmenu")=="1"
  then
    goto("aroma_installer_demo_pos");
  endif;

  ##########
  #
  # END DEBLOATER
  #

endif;


##
#
# Next Button is "Finish"
#
ini_set("text_next", "<~text_finish>");
ini_set("icon_next", "@finish");

#########################################################################################################
#
# STEP 6 - FINISH MESSAGE, Add Checkbox to Reboot or No. Save in "reboot_it" variable
#
# You can use return value or "reboot_it" variable to retrive checkbox value
# Example:
# if (checkviewbox(....)=="1" then
#    ... checked ...
# endif;
#
checkviewbox(
  #-- Title
    "<~finished.title>",
  
  #-- Text
    "<~finished.text1>\n\n<b>" +ini_get("rom_name")+ "<~finished.text2>\n\n"+
    "<~finished.installer>: "+getvar("retstatus")+"\n\n",
	
  #-- Icon
    "@welcome",

  #-- Checkbox Text
    "<~finished.reboot>",

  #-- Initial Checkbox value ( 0=unchecked, 1=checked ) -  (Optional, default:0)
    "1",

  #-- Save checked value in variable "reboot_it" (Optional)
    "reboot_it"
);

###
#
# Check if reboot checkbox was checked
if
  getvar("reboot_it")=="1"
then
  #
  # reboot("onfinish");   - Reboot if anything finished
  # reboot("now");        - Reboot Directly
  # reboot("disable");    - If you set reboot("onfinish") before, use this command to revert it.
  #
  reboot("onfinish");
endif;

#---- FINISH

  );

  ##
  #
  # Next Screen is Installation UI, So change the next button text to "Install Now"
  #
  ini_set("text_next", "<~soundcustomize.nextbutton>");
  ini_set("icon_next", "@installbutton");

  ###################################################################################################################
  #
  # DEBLOATER STEP 3 - MAKE SURE USER KNOW THAT NEXT STEP WAS INSTALLATION PROCESS
  #
  viewbox(
    #-- Title
      "<~soundconfirmation.title>",

    #-- Text
      "<~soundconfirmation.text>",

    #-- Icon
      "@install"
  );

  ##
  #
  # Next Button in successfull installation should be back to "Next >"
  #
  ini_set("text_next", "<~text_next>");
  ini_set("icon_next", "@next");

  #-- Extract sleep to /tmp/aroma/sleep. It will be used in updater-script for testing wait script
  restotmp("exec_demo/sleep","sleep");

  ###################################################################################################################
  #
  # SOUND STEP 4 - INSTALLATION PROCESS
  #
  setvar("retstatus",
    install(
	  #-- Title
        "<~soundinstall.title>",
	
	  #-- Installation Process message
	    "<b>"+ini_get("rom_name")+"</b> <~soundinstall.desc>",
	
	  #-- Installation Icon
        "@install",
	
	  #-- Installation Finish Message
        "<~soundinstall.finish>"
    )
  );

  ###################################################################################################################
  #
  # SOUND STEP 5 - ASK If We Want to go back to main menu
  #
  checkviewbox(
    #-- Title
      "<~soundpostinstall.title>",
  
    #-- Text
      "<~soundpostinstall.text>",
	
    #-- Icon
      "@finish",

    #-- Checkbox Text
      "<~soundpostinstall.checkbox>",

    #-- Initial Checkbox value ( 0=unchecked, 1=checked ) -  (Optional, default:0)
      "0",

    #-- Save checked value in variable "reboot_it" (Optional)
      "goto_mainmenu"
  );
  if
    getvar("goto_mainmenu")=="1"
  then
    goto("aroma_installer_demo_pos");
  endif;

  ##########
  #
  # END SOUND
  #

endif;


##--------------------------------------
#
# Debloater / selected 1 - if necessary?
#
if prop("demo.prop","selected")=="1" then
  #########################################################################################################
  #                                                                                                       #
  #                                           DEBLOATER                                                   #
  #                                                                                                       #
  #########################################################################################################

  #########################################################################################################
  #
  # DEBLOATER STEP 1 - INSTRUCTIONS
  #
  textbox(
    #-- Title
      "<~debloatinstructions.title>",
  
    #-- Subtitle
      "<~debloatinstructions.desc>",
  
    #-- Icon
      "@alert",
  
    #-- Arg 4
      "<~debloatinstructions.warning>"
